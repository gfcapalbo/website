<?xml version="1.0" encoding="utf-8"?>

<openerp>
  <data>

    <record id="bwi_menu_tree" model="ir.ui.view">
      <field name="name">BWI website menu tree</field>
      <field name="model">website.menu</field>
      <field name="inherit_id" ref="website.menu_tree"/>
      <field name="arch" type="xml">

        <field name="url" position="after">
          <field name="cms_page_id" />
        </field>

      </field>
    </record>

    <record id="bwi_menu_search" model="ir.ui.view">
      <field name="name">BWI website menu search</field>
      <field name="model">website.menu</field>
      <field name="inherit_id" ref="website.menu_search"/>
      <field name="arch" type="xml">
        <field name="url" position="after">
          <field name="cms_page_id" />
        </field>
      </field>
    </record>

  </data>
</openerp>
